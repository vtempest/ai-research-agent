{
  "entryPoints": [ "./src/**/*"],
  "tsconfig": "./tsconfig.json",
  "out": "./docs",
  "entryPointStrategy": "resolve",
  "navigation": {
    "includeCategories": false,
    "includeGroups": false,
    "includeFolders": true
  },
  "commentStyle":"all",
  "skipErrorChecking": true,
  "hideGenerator": true,
  "jsDocCompatibility": {
    "exampleTag": true,
    "defaultTag": true,
    "inheritDocTag": true,
    "ignoreUnescapedBraces": true
  },
  "sort": ["source-order"],
  "searchInDocuments": true,
  "searchInComments": true,
  "sidebarLinks": {
    "Source Code": "https://github.com/vtempest/",
  },
  
  "excludePrivate": true
}
