    "@typescript-eslint/parser": "8.33.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "buffer": "^6.0.3",
    "cmdk-sv": "^0.0.19",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.27.0",
