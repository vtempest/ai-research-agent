{
  "name": "qwksearch-web-app",
  "type": "module",
  "version": "0.9.9",
  "description": "Search, extract, vectorize, outline graph, and monitor web research",
  "author": "vtempest <grokthiscontact@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 vite dev",
    "serve": "NODE_NO_WARNINGS=1 vite build; wrangler deploy;  wrangler tail",
    "make": "NODE_NO_WARNINGS=1 vite build",
    "logs": " wrangler tail",
    "preview": " wrangler dev --live-reload",
    "test": "vitest",
    "db:generate": "drizzle-kit generate  --config=./src/lib/server/drizzle.config.ts",
    "db:push:dev": "wrangler d1 migrations apply qwksearch-db --local",
    "db:push:prod": "wrangler d1 migrations apply  qwksearch-db --remote",
    "db:studio": "ENV=dev drizzle-kit studio  --config=./src/lib/db/drizzle.config.ts",
    "db:backup": "wrangler d1 export qwksearch-db --output backup-prod.sql --remote",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241216.0",
    "@mxssfd/typedoc-theme": "^1.1.7",
    "@sveltejs/adapter-cloudflare": "^4.8.0",
    "@sveltejs/kit": "^2.12.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "9.6.1",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-plugin-svelte": "2.46.1",
    "postcss-load-config": "^6.0.1",
    "svelte-check": "^4.1.1",
    "typedoc": "^0.27.5",
    "typedoc-material-theme": "^1.2.0",
    "typedoc-plugin-ga": "^1.0.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8",
    "wrangler": "^3.103.2"
  },
  "dependencies": {
    "@auth/d1-adapter": "^1.7.4",
    "@auth/drizzle-adapter": "^1.7.4",
    "@auth/sveltekit": "^1.7.4",
    "@libsql/client": "^0.14.0",
    "@noble/hashes": "^1.6.1",
    "@sveltejs/adapter-cloudflare-workers": "^2.7.0",
    "@tanstack/svelte-query": "^5.64.2",
    "@tsparticles/slim": "3.7.1",
    "@tsparticles/svelte": "3.1.1",
    "@types/chrome": "^0.0.287",
    "arctic": "^2.3.3",
    "bits-ui": "^1.0.0-next.78",
    "check-password-strength": "^2.0.10",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.38.2",
    "formsnap": "^2.0.0",
    "highlight.js": "^11.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-svelte": "^0.473.0",
    "mode-watcher": "^0.5.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.49",
    "resend": "4.0.1",
    "sortablejs": "^1.15.6",
