{
  "name": "qwksearch-web-app",
  "type": "module",
  "version": "1.0.0",
  "description": "Search, extract, vectorize, outline graph, and monitor web research",
  "author": "vtempest <grokthiscontact@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 vite dev",
    "serve": "NODE_NO_WARNINGS=1 vite build; bunx wrangler deploy; bunx wrangler tail",
    "make": "NODE_NO_WARNINGS=1 vite build",
    "logs": "bunx wrangler tail",
    "preview": "bunx wrangler dev --live-reload",
    "test": "vitest",
    "db:generate": "drizzle-kit generate  --config=./src/lib/server/drizzle.config.ts",
    "db:push:dev": "wrangler d1 migrations apply qwksearch-db --local",
    "db:push:prod": "wrangler d1 migrations apply  qwksearch-db --remote",
    "db:studio": "ENV=dev drizzle-kit studio  --config=./src/lib/db/drizzle.config.ts",
    "db:backup": "wrangler d1 export qwksearch-db --output backup-prod.sql --remote",
    "format": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250422.0",
    "@mxssfd/typedoc-theme": "^1.1.7",
    "@sveltejs/adapter-cloudflare": "^7.0.1",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "9.6.1",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@typescript-eslint/parser": "8.31.0",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "cmdk-sv": "^0.0.19",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-svelte": "3.5.1",
    "postcss-load-config": "^6.0.1",
    "svelte-check": "^4.1.6",
    "typedoc": "^0.28.3",
    "typedoc-material-theme": "^1.4.0",
    "typedoc-plugin-ga": "^1.0.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.1.2",
    "wrangler": "^4.12.1"
  },
  "dependencies": {
    "@auth/d1-adapter": "^1.9.0",
    "@auth/drizzle-adapter": "^1.9.0",
    "@auth/sveltekit": "^1.9.0",
    "@libsql/client": "^0.15.4",
    "@noble/hashes": "^1.8.0",
    "@sveltejs/adapter-cloudflare-workers": "^2.9.0",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/svelte-query": "^5.74.4",
    "@tsparticles/slim": "3.8.1",
    "@tsparticles/svelte": "3.1.1",
    "@types/chrome": "^0.0.316",
    "arctic": "^3.6.0",
    "bits-ui": "0.22.0",
    "check-password-strength": "^3.0.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.42.0",
    "formsnap": "^2.0.1",
    "highlight.js": "^11.11.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-svelte": "^0.503.0",
    "mode-watcher": "^1.0.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.3",
    "resend": "4.4.0",
    "sortablejs": "^1.15.6",
    "stripe": "^18.0.0",
    "svelte": "^5.28.2",
    "svelte-dnd-action": "^0.9.60",
    "svelte-lightbox": "^1.1.6",
