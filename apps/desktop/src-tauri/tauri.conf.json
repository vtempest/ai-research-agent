{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../build",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  },
  "productName": "qwksearch",
  "version": "0.2.0",
  "identifier": "com.qwksearch.app",
  "plugins": {
    "shell": {
      "open": false
    }
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "QwkSearch",
        "width": 10,
        "height": 10,
        "visible": false
      }
    ]
  }
}